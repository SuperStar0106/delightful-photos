{
  "name": "fotos",
  "version": "0.8.2",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "test:e2e": "detox test -c ios.sim.debug",
    "build:e2e": "detox build -c ios.sim.debug",
    "ci:test:e2e": "detox test -c ios.sim.release -l verbose --cleanup",
    "ci:build:e2e": "detox build -c ios.sim.release",
    "compile": "tsc --noEmit -p . --pretty",
    "format": "npm-run-all format:*",
    "format:json": "npx prettier --write \"app/**/*.json\"",
    "format:md": "npx prettier --write \"**/*.md\"",
    "format:ts": "npx prettier --write \"app/**/*.ts{,x}\"",
    "format:test": "npx prettier --write \"test/**/*.ts\"",
    "lint": "npx eslint index.js app --fix --ext .js,.tsx ",
    "patch": "patch-package",
    "storybook": "start-storybook -p 9001 -c ./storybook",
    "test": "jest",
    "adb": "adb reverse tcp:9090 tcp:9090 && adb reverse tcp:3000 tcp:3000 && adb reverse tcp:9001 tcp:9001 && adb reverse tcp:8081 tcp:8081",
    "postinstall": "node ./bin/postInstall",
    "build-ios": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "build-android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "clean": "react-native-clean-project",
    "clean-all": "npx react-native clean-project-auto",
    "prepare": "husky install"
  },
  "dependencies": {
    "@functionland/fula-sec": "0.6.6",
    "@react-native-async-storage/async-storage": "^1.17.6",
    "@react-native-camera-roll/camera-roll": "github:ghorbani-m/react-native-cameraroll.git",
    "@react-native-community/masked-view": "0.1.10",
    "@react-native-community/netinfo": "^8.3.1",
    "@react-navigation/bottom-tabs": "^6.3.1",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/native-stack": "^6.6.2",
    "@react-navigation/stack": "^6.2.1",
    "@rneui/base": "^4.0.0-rc.2",
    "@rneui/themed": "^4.0.0-rc.2",
    "@types/lodash": "^4.14.182",
    "@types/shortid": "^0.0.29",
    "@types/uuid": "^8.3.4",
    "@walletconnect/react-native-dapp": "^1.7.8",
    "big-integer": "^1.6.51",
    "eslint-import-resolver-typescript": "^3.5.1",
    "expo": "^44.0.6",
    "expo-av": "~10.2.0",
    "expo-image-manipulator": "^10.1.2",
    "expo-linear-gradient": "~11.0.3",
    "expo-localization": "~12.0.0",
    "expo-media-library": "^14.0.1",
    "fula-recyclerlistview": "^3.1.0-beta.7",
    "i18n-js": "3.8.0",
    "lodash": "^4.17.21",
    "lottie-ios": "3.2.3",
    "lottie-react-native": "^5.0.1",
    "moment": "^2.29.4",
    "react": "17.0.2",
    "react-native": "0.66.3",
    "react-native-background-actions": "^2.6.7",
    "react-native-crypto": "^2.1.0",
    "react-native-fast-image": "^8.5.11",
    "react-native-gesture-handler": "^2.4.1",
    "react-native-get-random-values": "^1.8.0",
    "react-native-keychain": "^8.0.0",
    "react-native-permissions": "^3.6.1",
    "react-native-randombytes": "^3.6.1",
    "react-native-reanimated": "^2.8.0",
    "react-native-reanimated-carousel": "^3.1.0",
    "react-native-redash": "^16.3.0",
    "react-native-responsive-screen": "^1.4.2",
    "react-native-safe-area-context": "^4.1.2",
    "react-native-screens": "^3.13.1",
    "react-native-shared-element": "^0.8.4",
    "react-native-svg": "^12.3.0",
    "react-native-toast-message": "^2.1.5",
    "react-native-vector-icons": "^9.0.0",
    "react-navigation-collapsible": "^6.3.0",
    "react-navigation-shared-element": "^3.1.3",
    "reactotron-mst": "3.1.3",
    "reactotron-react-js": "^3.3.7",
    "reactotron-react-native": "5.0.0",
    "realm": "10.20.0-alpha.2",
    "recoil": "^0.6.1",
    "shortid": "^2.2.16",
    "text-encoding-polyfill": "^0.6.7",
    "uuid": "^8.3.2",
    "validate.js": "0.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-decorators": "7.12.1",
    "@babel/plugin-proposal-export-namespace-from": "^7.16.5",
    "@babel/plugin-proposal-optional-catch-binding": "7.12.1",
    "@babel/runtime": "^7.12.5",
    "@storybook/addon-storyshots": "6.1.10",
    "@storybook/react-native": "5.3.23",
    "@storybook/react-native-server": "5.3.23",
    "@types/i18n-js": "3.0.3",
    "@types/jest": "26.0.19",
    "@types/react": "^18.0.20",
    "@types/react-native": "^0.70.0",
    "@types/react-test-renderer": "17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.2",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "detox": "17.14.5",
    "eslint": "^8.23.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "16.0.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "fbjs-scripts": "3.0.0",
    "husky": "^8.0.0",
    "jest": "^25.5.4",
    "jest-circus": "25.5.4",
    "jest-expo": "^40.0.1",
    "jetifier": "1.6.6",
    "lint-staged": "^13.0.3",
    "node-libs-react-native": "^1.2.1",
    "npm-run-all": "4.1.5",
    "patch-package": "6.2.2",
    "postinstall-prepare": "1.0.1",
    "prettier": "^2.7.1",
    "react-devtools-core": "4.10.1",
    "react-dom": "^17.0.2",
    "react-native-clean-project": "^3.6.3",
    "react-native-web": "^0.16.3",
    "react-powerplug": "1.0.0",
    "solidarity": "^3.0.4",
    "typescript": "^4.8.3"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --quiet --cache --fix"
    ],
    "*.{json,js,ts,jsx,tsx,html}": [
      "prettier --write"
    ]
  },
  "jest": {
    "preset": "jest-expo",
    "setupFiles": [
      "<rootDir>/test/setup.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/e2e"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|expo|expo-linear-gradient|@react-native|@react-native-async-storage|@react-navigation|@storybook|@react-native-community|expo-localization)"
    ]
  },
  "detox": {
    "test-runner": "jest",
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/fotos.app",
        "build": "xcodebuild -workspace ios/fotos.xcworkspace -scheme fotos -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build -destination 'name=iPhone 11'",
        "type": "ios.simulator",
        "device": {
          "name": "iPhone 11",
          "os": "iOS 13.2"
        }
      },
      "ios.sim.release": {
        "binaryPath": "ios/build/Build/Products/Release-iphonesimulator/fotos.app",
        "build": "xcodebuild -workspace ios/fotos.xcworkspace -scheme fotos -configuration Release -sdk iphonesimulator -derivedDataPath ios/build -destination 'name=iPhone 11'",
        "type": "ios.simulator",
        "device": {
          "name": "iPhone 11",
          "os": "iOS 13.2"
        }
      }
    }
  },
  "resolutions": {
    "react-native-svg": "^12.3.0"
  },
  "expo": {
    "autolinking": {
      "ios": {
        "exclude": [
          "expo-font"
        ]
      }
    }
  }
}
